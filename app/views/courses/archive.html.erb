<p>Archive page</p>


<p>Current:</p>
<% @courses.each do |course| %>
  <% if course.is_archived? == false %> 
    <div>
      <%= course.name %>
      <hr>
    </div>
    <% end %>
<% end %>

<p>Archived:</p>
  <% @courses.each do |course| %>
  <% if course.is_archived? == true %>
    <div>
      <%= course.name %>
      <hr>
    </div>
  <% end %>
<% end %>
